{
  "name": "nuttx-dev-pack",
  "displayName": "NuttX Dev Pack",
  "description": "VS Code extension pack for NuttX developper.",
  "repository": {
    "type": "git",
    "url": "https://github.com/rwb-nuttx/vsce_nuttx_dev_pack"
  },
  "publisher": "NuttX",
  "version": "0.1.5",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "build": "npx vsce package",
    "lint": "eslint src --ext ts",
    "publish": "npx vsce publish"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Extension Packs"
  ],
  "main": "./out/extension.js",
  "extensionPack": [
    "luveti.kconfig",
    "ms-vscode.makefile-tools",
    "Chouzz.vscode-better-align",
    "alefragnani.bookmarks"
  ],
  "contributes": {
    "commands": [
      {
        "command": "nuttx-dev-pack.run",
        "title": "NuttX Dev Pack Run"
      },
      {
				"command": "nuttx-dev-pack.align_sort",
				"title": "NuttX Dev Pack align and sort codes"
			}
    ],
    "keybindings": [
      {
        "command": "nuttx-dev-pack.align_sort",
        "key": "ctrl+alt+a",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/python.code-snippets"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^2.18.0",
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@fabiospampinato/bump": "^2.1.2",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {}
}